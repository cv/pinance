{
	"name": "pinance",
	"version": "0.1.1",
	"description": "pi extension for financial data tools (crypto, stocks, SEC filings, etc.)",
	"type": "module",
	"private": true,
	"scripts": {
		"check": "biome check .",
		"check:fix": "biome check --write .",
		"test": "vitest run --exclude src/e2e.test.ts",
		"test:e2e": "vitest run src/e2e.test.ts",
		"test:all": "vitest run",
		"test:watch": "vitest --exclude src/e2e.test.ts",
		"typecheck": "tsc --noEmit",
		"prepare": "husky"
	},
	"pi": {
		"extensions": [
			"./src/index.ts"
		]
	},
	"keywords": [
		"pi",
		"pi-extension",
		"finance",
		"stocks",
		"crypto",
		"sec-filings"
	],
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "^2.3.12",
		"@sinclair/typebox": "^0.34.47",
		"@types/node": "^25.0.9",
		"@vitest/coverage-v8": "^4.0.17",
		"dotenv": "^17.2.3",
		"husky": "^9.1.7",
		"typescript": "^5.9.3",
		"vitest": "^4.0.17"
	},
	"peerDependencies": {
		"@mariozechner/pi-coding-agent": "*"
	}
}
