{
	"name": "pinance",
	"version": "0.1.0",
	"description": "pi extension for financial data tools (crypto, stocks, SEC filings, etc.)",
	"type": "module",
	"private": true,
	"scripts": {
		"check": "biome check .",
		"check:fix": "biome check --write .",
		"test": "vitest run",
		"test:watch": "vitest",
		"typecheck": "tsc --noEmit",
		"prepare": "husky"
	},
	"pi": {
		"extensions": [
			"./src/index.ts"
		]
	},
	"keywords": [
		"pi",
		"pi-extension",
		"finance",
		"stocks",
		"crypto",
		"sec-filings"
	],
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@sinclair/typebox": "^0.34.14",
		"@types/node": "^25.0.6",
		"husky": "^9.1.7",
		"typescript": "^5.7.3",
		"vitest": "^4.0.16"
	},
	"peerDependencies": {
		"@mariozechner/pi-coding-agent": "*"
	},
	"dependencies": {
		"dotenv": "^17.2.3"
	}
}
